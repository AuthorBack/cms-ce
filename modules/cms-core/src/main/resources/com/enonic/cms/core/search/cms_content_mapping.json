{
    "content":{
        "_all":{
            "enabled":false
        },
        "_source":{
            "enabled":false
        },
        "date_detection":false,
        "numeric_detection":false,
        "properties":{
            "key":{
                "type":"double",
                "store":"yes"
            },
            "_all_userdata":{
                "type":"multi_field",
                "fields":{
                    "_all_userdata":{
                        "include_in_all":false,
                        "type":"string",
                        "index":"not_analyzed",
                        "store":"yes"
                    },
                    "_tokenized":{
                        "include_in_all":false,
                        "type":"string",
                        "index":"analyzed",
                        "store":"yes"
                    }
                }
            },
            "title":{
                "type":"multi_field",
                "fields":{
                    "title":{
                        "include_in_all":false,
                        "type":"string",
                        "index":"not_analyzed",
                        "store":"yes"
                    },
                    "_tokenized":{
                        "include_in_all":false,
                        "type":"string",
                        "index":"analyzed",
                        "store":"yes"
                    }
                }
            },
            "timestamp":{
                "include_in_all":false,
                "type":"date",
                "index":"not_analyzed",
                "store":"yes"
            },
            "publishfrom":{
                "type":"date",
                "index":"not_analyzed",
                "store":"yes"
            },
            "publishto":{
                "type":"date",
                "index":"not_analyzed",
                "store":"yes"
            },
            "assignmentduedate":{
                "type":"date",
                "index":"not_analyzed",
                "store":"yes"
            },
            "status":{
                "type":"double",
                "store":"yes"
            },
            "priority":{
                "type":"double",
                "store":"yes"
            },
            "categorykey":{
                "type":"double",
                "store":"yes"
            },
            "contenttypekey":{
                "type":"double",
                "store":"yes"
            },
            "contentlocations_approved":{
                "type":"double",
                "store":"yes"
            },
            "contentlocations_unapproved":{
                "type":"double",
                "store":"yes"
            }
        },
        "dynamic_templates":[
            {
                "template_userdata_date":{
                    "match":"*.date",
                    "mapping":{
                        "type":"date",
                        "store":"yes"
                    }
                }
            },
            {
                "template_userdata_number":{
                    "match":"*.number",
                    "mapping":{
                        "type":"double",
                        "store":"yes"
                    }
                }
            },
            {
                "template_userdata_strings":{
                    "match":"*",
                    "mapping":{
                        "type":"multi_field",
                        "fields":{
                            "{name}":{
                                "type":"string",
                                "index":"not_analyzed",
                                "store":"yes"
                            },
                            "_tokenized":{
                                "type":"string",
                                "index":"analyzed",
                                "store":"yes"
                            }
                        }
                    }
                }
            }
        ]
    }
}
