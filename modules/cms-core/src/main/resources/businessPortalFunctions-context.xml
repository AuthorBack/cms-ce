<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="createAttachmentUrclFunction" class="com.enonic.cms.business.portal.rendering.portalfunctions.CreateAttachmentUrlFunction">
    <property name="contentDao" ref="contentDao"/>
  </bean>

  <bean id="isWindowEmptyFunction" class="com.enonic.cms.business.portal.rendering.portalfunctions.IsWindowEmptyFunction">
    <property name="windowRendererFactory" ref="windowRendererFactory"/>
  </bean>

</beans>
    
